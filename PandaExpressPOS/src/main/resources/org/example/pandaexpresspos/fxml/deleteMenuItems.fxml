<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.VBox?>

<?import javafx.scene.layout.HBox?>
<VBox xmlns:fx="http://javafx.com/fxml/1" xmlns="http://javafx.com/javafx/21" fx:controller="org.example.pandaexpresspos.SceneController">
    <!-- Other components can go here -->

    <!-- VBox where the buttons will be dynamically added -->
    <HBox fx:id="buttonContainer" alignment="CENTER" spacing="10">
        <!-- Initially empty, buttons will be added dynamically from the controller -->
    </HBox>
    <AnchorPane prefHeight="346.0" prefWidth="8535.0">
        <children>
            <Button layoutX="109.0" layoutY="21.0" mnemonicParsing="false" onAction="#switchToScene1" text="Back" />
        </children>
    </AnchorPane>
</VBox>
